{{! public/pages/profile }}
<div class="row profile public">
  <div class="large-8 columns">
    <div class="row">
      <div class="columns profile-header">
        <div class="profile-badge">
          <div class="image"><img src="/static/img/profile.png"></div>
        </div>
        <div class="profile-meta">
          <h1 class="username">
            gary menezes
          </h1>
        </div>
        <ul class="profile-menu">
          <li class="active"><h5 class="title">Entries</h5></li>
          <li class="inactive"><h5 class="title">Stats</h5></li>
          <li class="inactive"><h5 class="title">Arts</h5></li>
          <li class="inactive"><h5 class="title">Locations</h5></li>
        </ul>
      </div>

      <div class="columns end profile-feed">
        <ul class="entry-list">
          {{#each entries }}
            <li>
              <div class="entry-right">
                <a class="link" href="{{ link }}">view</a>
              </div>
              <h2 class="title entry-left">{{> partials/rating }} {{ art.name }} {{ type.name }}</h2>
              <div class="date entry-left">{{ date }}</div>
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
  
  <div class="small-10 large-4 columns end comments">
  comments
  </div>
</div>